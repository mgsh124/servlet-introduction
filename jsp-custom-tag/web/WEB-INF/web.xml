<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>com.mum.MainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BeerSelect</servlet-name>
        <servlet-class>com.mum.BeerSelect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BeerSelect</servlet-name>
        <url-pattern>/BeerSelect</url-pattern>
        <url-pattern>/SelectBeer.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CustomTagServlet</servlet-name>
        <servlet-class>com.mum.CustomTagServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomTagServlet</servlet-name>
        <url-pattern>/CustomTagServlet</url-pattern>
    </servlet-mapping>
</web-app>